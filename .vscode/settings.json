{
    "go.lintTool": "golangci-lint",
    "go.lintOnSave": "package",
    "go.formatTool": "goimports",
    "go.autocompleteUnimportedPackages": true,
    "git.ignoreLimitWarning": true,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pub",
        "python",
        "restructuredtext",
        "rust",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "cSpell.enabled": true,
    "cSpell.words": [
        "Debugf",
        "Infof",
        "Jinmu",
        "deckarep",
        "godotenv",
        "gorm",
        "jinmuhealth",
        "jinmukeji",
        "jinzhu",
        "joho",
        "kataras",
        "mysqldb",
        "nolint",
        "protobuf",
        "ptypes",
        "stretchr",
        "uuid",
        "wechat",
        "deregisters",
        "fqdn"
    ],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "todo-tree.tree.expanded": true,
    "todo-tree.tree.flat": true
}
